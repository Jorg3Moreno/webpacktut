{
  "name": "webpacktut",
  "version": "1.0.0",
  "description": "Webpack tutorial",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:entries": "webpack --config ./multi-entries/webpack.config.js",
    "build:css-loader": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jorg3Moreno/webpacktut.git"
  },
  "keywords": [
    "webpack",
    "javascript",
    "js"
  ],
  "author": "@Jorg3Moreno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jorg3Moreno/webpacktut/issues"
  },
  "homepage": "https://github.com/Jorg3Moreno/webpacktut#readme",
  "dependencies": {},
  "devDependencies": {
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.12"
  }
}
